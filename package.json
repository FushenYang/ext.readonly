{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "readonly",
  "title": "ReadOnly",
  "description": "进行文件只读标记/或者取消文件只读标记的工具。",
  "icon": "command-icon.png",
  "author": "yangfs",
  "categories": [
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "readonly",
      "description": "选中的文件修改为只读或者取消只读选项。",
      "mode": "no-view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.49.3",
    "@raycast/utils": "^1.4.8"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/jest": "^29.5.1",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "jest": "^29.5.0",
    "prettier": "^2.5.1",
    "ts-jest": "^29.1.0",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "**/*.test.ts"
    ],
    "preset": "ts-jest"
  }
}